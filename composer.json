{
    "name": "silverstripeltd/bambusa-installer",
    "type": "silverstripe-recipe",
    "description": "Bambusa Project Template",
    "license": "BSD-3-Clause",
    "repositories": [
    	{
    		"url": "https://github.com/silverstripeltd/bambusa-theme",
    		"type": "vcs"
    	}
    ],
    "require": {
        "php": ">=7.2.0",
        "silverstripe/recipe-plugin": "^1",
        "silverstripe/recipe-cms": "^4.4.1",
        "silverstripe/bambusa-theme": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^3"
    },
    "extra": {
        "project-files": ["app/*"]
    },
    "config": {
        "process-timeout": 600
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
